{
	"name": "parbez",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'app/**/*.tsx' --out-file ./app/unocss.css",
		"dev": "concurrently --kill-others 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@tabler/icons-react": "^2.42.0",
		"@unocss/reset": "^0.57.7",
		"@vercel/analytics": "^1.1.1",
		"next": "14.0.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sharp": "^0.32.6"
	},
	"devDependencies": {
		"@types/node": "20.10.0",
		"@types/react": "18.2.38",
		"@types/react-dom": "18.2.17",
		"@unocss/cli": "^0.57.7",
		"@unocss/eslint-config": "^0.57.7",
		"concurrently": "^8.2.2",
		"eslint": "8.54.0",
		"eslint-config-mahir": "^0.0.36",
		"prettier": "^3.1.0",
		"typescript": "5.3.2",
		"unocss": "^0.57.7"
	},
	"packageManager": "yarn@4.0.2"
}
