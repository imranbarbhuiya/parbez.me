{
	"name": "parbez",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'app/**/*.tsx' --out-file ./app/unocss.css",
		"dev": "concurrently --kill-others 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@tabler/icons-react": "^2.22.0",
		"@unocss/reset": "^0.53.1",
		"@vercel/analytics": "^1.0.1",
		"next": "13.4.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sharp": "^0.32.1"
	},
	"devDependencies": {
		"@types/node": "20.2.3",
		"@types/prettier": "^2.7.3",
		"@types/react": "18.2.11",
		"@types/react-dom": "18.2.4",
		"@types/sharp": "^0.32.0",
		"@unocss/cli": "^0.53.1",
		"@unocss/eslint-config": "^0.53.1",
		"concurrently": "^8.2.0",
		"eslint": "8.42.0",
		"eslint-config-mahir": "^0.0.26",
		"prettier": "^2.8.8",
		"typescript": "5.1.3",
		"unocss": "^0.53.1"
	},
	"packageManager": "yarn@3.6.0"
}
