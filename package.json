{
	"name": "parbez",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'app/**/*.tsx' --out-file ./app/unocss.css",
		"dev": "concurrently --kill-others 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@tabler/icons-react": "^2.47.0",
		"@unocss/reset": "^0.58.4",
		"@vercel/analytics": "^1.1.3",
		"next": "14.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sharp": "^0.33.2"
	},
	"devDependencies": {
		"@types/node": "20.11.17",
		"@types/react": "18.2.55",
		"@types/react-dom": "18.2.19",
		"@unocss/cli": "^0.58.4",
		"@unocss/eslint-config": "^0.58.4",
		"concurrently": "^8.2.2",
		"eslint": "8.56.0",
		"eslint-config-mahir": "^0.0.38",
		"prettier": "^3.2.5",
		"typescript": "5.3.3",
		"unocss": "^0.58.4"
	},
	"packageManager": "yarn@4.1.0"
}
