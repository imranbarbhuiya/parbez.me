{
	"name": "parbez",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'app/**/*.tsx' --out-file ./app/unocss.css",
		"dev": "concurrently --kill-others 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@tabler/icons-react": "^2.31.0",
		"@unocss/reset": "^0.55.2",
		"@vercel/analytics": "^1.0.2",
		"next": "13.4.19",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sharp": "^0.32.5"
	},
	"devDependencies": {
		"@types/node": "20.5.1",
		"@types/prettier": "^2.7.3",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@types/sharp": "^0.32.0",
		"@unocss/cli": "^0.55.2",
		"@unocss/eslint-config": "^0.55.2",
		"concurrently": "^8.2.0",
		"eslint": "8.47.0",
		"eslint-config-mahir": "^0.0.31",
		"prettier": "^3.0.2",
		"typescript": "5.1.6",
		"unocss": "^0.55.2"
	},
	"packageManager": "yarn@3.6.2"
}
