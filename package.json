{
	"name": "parbez",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'app/**/*.tsx' --out-file ./app/unocss.css",
		"dev": "concurrently --kill-others 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@tabler/icons-react": "^3.12.0",
		"@unocss/reset": "^0.62.2",
		"@vercel/analytics": "^1.3.1",
		"next": "14.2.5",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@types/node": "20.15.0",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@unocss/cli": "^0.62.2",
		"@unocss/eslint-config": "^0.62.2",
		"concurrently": "^8.2.2",
		"eslint": "8.57.0",
		"eslint-config-mahir": "^0.0.45",
		"prettier": "^3.3.3",
		"typescript": "5.5.4",
		"unocss": "^0.62.2"
	},
	"packageManager": "yarn@4.4.0"
}
